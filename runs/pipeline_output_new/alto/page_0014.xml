  <Layout>
    <Page ID="p1" WIDTH="1684" HEIGHT="2467">
      <PrintSpace>
        <TextBlock ID="region_0_MainZone-Continued" HPOS="207" VPOS="341" WIDTH="1409" HEIGHT="1291" TAGREFS="MainZone-Continued">
          <TextLine HPOS="229" VPOS="349" WIDTH="1374" HEIGHT="46">
            <Baseline POINTS="229,386 680,386 864,382 1603,384"/>
            <String CONTENT="neratione, &amp; la corruttione delle tose naturali, o la dispositione de Cieli, o l’influen¬"/>
          </TextLine>
          <TextLine HPOS="229" VPOS="397" WIDTH="1373" HEIGHT="50">
            <Baseline POINTS="229,434 1602,432"/>
            <String CONTENT="za delle stelle, o la fermezza della Terra, o altre simili cose, le quali con vn lungo"/>
          </TextLine>
          <TextLine HPOS="228" VPOS="449" WIDTH="1375" HEIGHT="47">
            <Baseline POINTS="229,484 810,480 1603,482"/>
            <String CONTENT="studio ritrouarono per auanzare in questa cognitione la Plebe, &amp; accioche non egual"/>
          </TextLine>
          <TextLine HPOS="228" VPOS="498" WIDTH="1373" HEIGHT="48">
            <Baseline POINTS="229,532 889,529 1601,533"/>
            <String CONTENT="mente i dotti, &amp; l’ignoranti potessero intendere, &amp; penetrare le tagioni delle cose, se"/>
          </TextLine>
          <TextLine HPOS="228" VPOS="548" WIDTH="1375" HEIGHT="46">
            <Baseline POINTS="229,581 795,578 1603,581"/>
            <String CONTENT="le andauano copertamente communicando fra loro, &amp; coperte ancora per mezzo di"/>
          </TextLine>
          <TextLine HPOS="227" VPOS="594" WIDTH="1373" HEIGHT="50">
            <Baseline POINTS="227,630 663,627 1600,629"/>
            <String CONTENT="queste imagini, le lasciauano a’posteri, che doueuano d'gli altri essere superiori di di¬"/>
          </TextLine>
          <TextLine HPOS="229" VPOS="644" WIDTH="1371" HEIGHT="49">
            <Baseline POINTS="229,680 660,677 1600,679"/>
            <String CONTENT="gnità, &amp; di sapienza. Di qui è nata la moltitudine delle Fauole de gli antichi"/>
          </TextLine>
          <TextLine HPOS="224" VPOS="692" WIDTH="1371" HEIGHT="49">
            <Baseline POINTS="225,731 577,726 1595,729"/>
            <String CONTENT="Scrittori, le quali hanno l’vtile della scienza per li dotti, &amp; il dolce delle curiose"/>
          </TextLine>
          <TextLine HPOS="224" VPOS="742" WIDTH="1369" HEIGHT="49">
            <Baseline POINTS="224,779 491,775 1593,777"/>
            <String CONTENT="narrationi per gl’ignoranti. Però molti ancora de gli huomini di gran conto banno"/>
          </TextLine>
          <TextLine HPOS="224" VPOS="792" WIDTH="1371" HEIGHT="48">
            <Baseline POINTS="225,828 557,825 1595,826"/>
            <String CONTENT="stimato loro degna fatica lo spiegare quelle cose, che trouauano in queste Fauole oc¬"/>
          </TextLine>
          <TextLine HPOS="222" VPOS="840" WIDTH="1377" HEIGHT="48">
            <Baseline POINTS="223,876 1599,874"/>
            <String CONTENT="cultate, lasciandoci scritto, che per l’imagine di Saturno intendeuano il Tempo, il qua"/>
          </TextLine>
          <TextLine HPOS="221" VPOS="888" WIDTH="1378" HEIGHT="51">
            <Baseline POINTS="222,925 1599,922"/>
            <String CONTENT="le a gli anni, a' mesi, ed a' giorni dà, &amp; toglie l’essere, come eßso diuoraua quei me¬"/>
          </TextLine>
          <TextLine HPOS="220" VPOS="937" WIDTH="1378" HEIGHT="50">
            <Baseline POINTS="221,974 1598,972"/>
            <String CONTENT="desimi fanciulli, che erano suoi figliuoli. E per quella di Gioue fulminante, la par¬"/>
          </TextLine>
          <TextLine HPOS="219" VPOS="987" WIDTH="1377" HEIGHT="49">
            <Baseline POINTS="220,1023 1596,1021"/>
            <String CONTENT="te del Cielo più pura, donde vengono quasi tutti gli effetti Meteorologici. Per l’I¬"/>
          </TextLine>
          <TextLine HPOS="220" VPOS="1037" WIDTH="1375" HEIGHT="48">
            <Baseline POINTS="221,1072 1595,1070"/>
            <String CONTENT="magine ancora di Venere d’estrema bellezza, l’appetito della materia prima, come"/>
          </TextLine>
          <TextLine HPOS="219" VPOS="1083" WIDTH="1379" HEIGHT="49">
            <Baseline POINTS="219,1122 507,1119 1090,1122 1598,1119"/>
            <String CONTENT="dicono i Filosofi, alla forma, che le da il compimento. E che quelli, che credeuano il"/>
          </TextLine>
          <TextLine HPOS="222" VPOS="1134" WIDTH="1375" HEIGHT="49">
            <Baseline POINTS="222,1172 463,1168 1597,1169"/>
            <String CONTENT="Mondo essere torpo mobile, ed ogni cosa succedere per lo predominio delle Stelle ( se¬"/>
          </TextLine>
          <TextLine HPOS="222" VPOS="1184" WIDTH="1371" HEIGHT="47">
            <Baseline POINTS="222,1219 1593,1219"/>
            <String CONTENT="condo, che racconta nel Pimandro Mercurio Trismegisto) finsero Argo Pastorale,"/>
          </TextLine>
          <TextLine HPOS="221" VPOS="1231" WIDTH="1379" HEIGHT="50">
            <Baseline POINTS="222,1268 1600,1266"/>
            <String CONTENT="che con molti occhi da tutte le bande riguardasse. Questo istesso mostrarono in Giu¬"/>
          </TextLine>
          <TextLine HPOS="220" VPOS="1282" WIDTH="1377" HEIGHT="47">
            <Baseline POINTS="221,1317 1597,1316"/>
            <String CONTENT="none, sospesa in aria dalla mano di Gioue, come dißse Homero, ed infinite altre imagi"/>
          </TextLine>
          <TextLine HPOS="219" VPOS="1331" WIDTH="1381" HEIGHT="48">
            <Baseline POINTS="220,1367 1600,1363"/>
            <String CONTENT="ni, le quali hanno gia ripient molti volumi, &amp; stancati molti Scrittori, ma con pro¬"/>
          </TextLine>
          <TextLine HPOS="219" VPOS="1380" WIDTH="1379" HEIGHT="49">
            <Baseline POINTS="219,1416 1598,1415"/>
            <String CONTENT="fitto di dottrina, &amp; di sapienza. Il secondo modo delle Imagini abbraccia quelle cose"/>
          </TextLine>
          <TextLine HPOS="218" VPOS="1431" WIDTH="1374" HEIGHT="46">
            <Baseline POINTS="219,1463 1592,1465"/>
            <String CONTENT="che sono nell’huomo medesimo, o che hanno gran vicinanza con esso, come i concetti,"/>
          </TextLine>
          <TextLine HPOS="221" VPOS="1477" WIDTH="1378" HEIGHT="51">
            <Baseline POINTS="222,1514 1599,1511"/>
            <String CONTENT="&amp; gli habiti, che da' concetti ne nascono, con la frequenza di molte attioni partico¬"/>
          </TextLine>
          <TextLine HPOS="219" VPOS="1527" WIDTH="1378" HEIGHT="50">
            <Baseline POINTS="220,1561 1597,1560"/>
            <String CONTENT="lari; &amp; concetti dimandiamo senza più sottile inuestigatione, tutto quello, che può es¬"/>
          </TextLine>
          <TextLine HPOS="225" VPOS="1577" WIDTH="1339" HEIGHT="49">
            <Baseline POINTS="226,1610 1498,1610 1564,1613"/>
            <String CONTENT="ser significato con le parole; il qual tutto vien commodamente in due parti diuiso."/>
          </TextLine>
        </TextBlock>
        <TextBlock ID="region_1_MainZone-P" HPOS="207" VPOS="1626" WIDTH="1401" HEIGHT="646" TAGREFS="MainZone-P">
          <TextLine HPOS="278" VPOS="1633" WIDTH="1312" HEIGHT="38">
            <Baseline POINTS="278,1662 346,1660 508,1662 658,1661 1590,1661"/>
            <String CONTENT="L’vna parteè, che afferma, o nega qualche cola d’alcuno; l’altra, che no. Con"/>
          </TextLine>
          <TextLine HPOS="224" VPOS="1678" WIDTH="1364" HEIGHT="44">
            <Baseline POINTS="225,1709 890,1711 1516,1709 1588,1711"/>
            <String CONTENT="quella formano l’artifitio loro quelli, che compongono l’Imprese, nelle quali con pochi"/>
          </TextLine>
          <TextLine HPOS="222" VPOS="1729" WIDTH="1355" HEIGHT="39">
            <Baseline POINTS="223,1759 286,1758 334,1760 414,1758 955,1760 1013,1761 1065,1759 1577,1758"/>
            <String CONTENT="corpi, &amp; poche parole vn sol concetto s’accenna, &amp; quelli ancora, che fanno gli Em"/>
          </TextLine>
          <TextLine HPOS="224" VPOS="1774" WIDTH="1363" HEIGHT="43">
            <Baseline POINTS="225,1807 491,1806 664,1809 1130,1810 1343,1808 1587,1809"/>
            <String CONTENT="blemi, oue maggior concetto con più quantità di parole, &amp; di corpi si manifesta. Con"/>
          </TextLine>
          <TextLine HPOS="446" VPOS="1807" WIDTH="46" HEIGHT="17">
            <Baseline POINTS="446,1819 492,1819"/>
            <String CONTENT="10"/>
          </TextLine>
          <TextLine HPOS="223" VPOS="1825" WIDTH="1355" HEIGHT="45">
            <Baseline POINTS="223,1855 576,1856 775,1858 1477,1858 1578,1856"/>
            <String CONTENT="questa poi si forma l’arte dell’altre Imageni, le quali appartengono al nostro discor"/>
          </TextLine>
          <TextLine HPOS="224" VPOS="1878" WIDTH="14" HEIGHT="42">
            <Baseline POINTS="228,1916 238,1915"/>
            <String CONTENT="4"/>
          </TextLine>
          <TextLine HPOS="242" VPOS="1875" WIDTH="1340" HEIGHT="40">
            <Baseline POINTS="243,1904 700,1905 815,1907 1555,1906 1582,1908"/>
            <String CONTENT="o, per la conformita, che hanno con le definitioni; le quali solo abbracciano le virtil"/>
          </TextLine>
          <TextLine HPOS="270" VPOS="1911" WIDTH="7" HEIGHT="11">
            <Baseline POINTS="270,1916 277,1916"/>
            <String CONTENT="4"/>
          </TextLine>
          <TextLine HPOS="224" VPOS="1924" WIDTH="1353" HEIGHT="40">
            <Baseline POINTS="224,1954 354,1953 390,1955 536,1953 771,1956 1577,1956"/>
            <String CONTENT="ed i vitij, otutte quelle cose, che hanno conuenienza con questi, o con quelle, senze"/>
          </TextLine>
          <TextLine HPOS="222" VPOS="1971" WIDTH="1349" HEIGHT="46">
            <Baseline POINTS="223,2003 658,2002 840,2006 1274,2006 1400,2004 1454,2006 1571,2002"/>
            <String CONTENT="affermare, ô negare alcuna cosa, e per essere ò sole priuationt, o habiti puri, si espri"/>
          </TextLine>
          <TextLine HPOS="243" VPOS="2011" WIDTH="26" HEIGHT="10">
            <Baseline POINTS="244,2016 269,2015"/>
            <String CONTENT="6°."/>
          </TextLine>
          <TextLine HPOS="1506" VPOS="2010" WIDTH="33" HEIGHT="12">
            <Baseline POINTS="1506,2017 1539,2017"/>
            <String CONTENT="y4"/>
          </TextLine>
          <TextLine HPOS="222" VPOS="2018" WIDTH="1368" HEIGHT="50">
            <Baseline POINTS="222,2052 261,2051 606,2051 1006,2056 1110,2054 1199,2056 1274,2054 1590,2054"/>
            <String CONTENT="mono con la figura humana conuenientemente. Percioche, si come l’huomo tutto é"/>
          </TextLine>
          <TextLine HPOS="224" VPOS="2067" WIDTH="1028" HEIGHT="45">
            <Baseline POINTS="224,2101 685,2100 914,2104 1252,2104"/>
            <String CONTENT="misura di tutte le cose, secondo la commune opinione de' Filosofi"/>
          </TextLine>
          <TextLine HPOS="1279" VPOS="2074" WIDTH="34" HEIGHT="40">
            <Baseline POINTS="1280,2107 1313,2107"/>
            <String CONTENT="&amp;"/>
          </TextLine>
          <TextLine HPOS="1322" VPOS="2069" WIDTH="268" HEIGHT="41">
            <Baseline POINTS="1323,2106 1423,2103 1590,2103"/>
            <String CONTENT="s’Aristotele in"/>
          </TextLine>
          <TextLine HPOS="226" VPOS="2123" WIDTH="1361" HEIGHT="40">
            <Baseline POINTS="226,2149 668,2149 979,2153 1587,2151"/>
            <String CONTENT="particolare, quasi come la definitione è misura del definito, cosi medesimamente la for"/>
          </TextLine>
          <TextLine HPOS="223" VPOS="2149" WIDTH="16" HEIGHT="18">
            <Baseline POINTS="224,2163 239,2162"/>
            <String CONTENT="4"/>
          </TextLine>
          <TextLine HPOS="222" VPOS="2165" WIDTH="1370" HEIGHT="49">
            <Baseline POINTS="223,2199 612,2198 1151,2203 1323,2203 1443,2201 1592,2201"/>
            <String CONTENT="ma accidentale. che apparisce esteriormente d’esso, può esser misura accidentale delle"/>
          </TextLine>
          <TextLine HPOS="225" VPOS="2215" WIDTH="1362" HEIGHT="50">
            <Baseline POINTS="226,2247 352,2249 662,2248 845,2251 1256,2254 1587,2247"/>
            <String CONTENT="qualità definibili, qua lunque si stano, o dell’anima nostra sola, o di tutto il compo¬"/>
          </TextLine>
          <TextLine HPOS="741" VPOS="2251" WIDTH="6" HEIGHT="17">
            <Baseline POINTS="741,2261 747,2261"/>
            <String CONTENT=""/>
          </TextLine>
          <TextLine HPOS="1582" VPOS="2256" WIDTH="5" HEIGHT="13">
            <Baseline POINTS="1582,2265 1587,2265"/>
            <String CONTENT="¬"/>
          </TextLine>
