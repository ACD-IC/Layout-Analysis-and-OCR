        <TextBlock ID="region_0_MainZone-Continued" HPOS="92" VPOS="204" WIDTH="1437" HEIGHT="1357" TAGREFS="MainZone-Continued">
          <TextLine HPOS="119" VPOS="208" WIDTH="1392" HEIGHT="38">
            <Baseline POINTS="119,232 1511,230"/>
            <String CONTENT="gale delirlla vn pelzo dilaticato pel aitii, il portoliTi¬"/>
          </TextLine>
          <TextLine HPOS="119" VPOS="236" WIDTH="29" HEIGHT="17">
            <Baseline POINTS="120,247 148,246"/>
            <String CONTENT="O"/>
          </TextLine>
          <TextLine HPOS="116" VPOS="249" WIDTH="1389" HEIGHT="62">
            <Baseline POINTS="117,298 1505,295"/>
            <String CONTENT="poso desiderato da gl’animi, che seguono la traccia del ve¬"/>
          </TextLine>
          <TextLine HPOS="116" VPOS="295" WIDTH="22" HEIGHT="25">
            <Baseline POINTS="116,314 138,314"/>
            <String CONTENT="1"/>
          </TextLine>
          <TextLine HPOS="112" VPOS="317" WIDTH="1397" HEIGHT="62">
            <Baseline POINTS="113,365 1509,363"/>
            <String CONTENT="ro bene, non ingannati punto dall’instabili tà delle huma¬"/>
          </TextLine>
          <TextLine HPOS="113" VPOS="391" WIDTH="1396" HEIGHT="54">
            <Baseline POINTS="113,430 478,433 1131,430 1509,433"/>
            <String CONTENT="ne speranze. Per quanto poi à me tocca io ho riceuuto"/>
          </TextLine>
          <TextLine HPOS="113" VPOS="449" WIDTH="1397" HEIGHT="60">
            <Baseline POINTS="113,498 1075,496 1510,500"/>
            <String CONTENT="fauore da V. P. molto Reuerenda che lungo sarebbe il ri¬"/>
          </TextLine>
          <TextLine HPOS="113" VPOS="517" WIDTH="1396" HEIGHT="61">
            <Baseline POINTS="113,563 1509,565"/>
            <String CONTENT="dirli, &amp; basta, ch’io dica, che sono vinti, &amp; più anni, ch’el¬"/>
          </TextLine>
          <TextLine HPOS="111" VPOS="583" WIDTH="1399" HEIGHT="62">
            <Baseline POINTS="112,630 1510,633"/>
            <String CONTENT="la di continuo m’obliga alle sue cortesi maniere. Per tut¬"/>
          </TextLine>
          <TextLine HPOS="113" VPOS="648" WIDTH="1399" HEIGHT="67">
            <Baseline POINTS="114,698 1512,700"/>
            <String CONTENT="ti questi capi io mi sento si desideroso di mostrarli qual¬"/>
          </TextLine>
          <TextLine HPOS="113" VPOS="718" WIDTH="1399" HEIGHT="66">
            <Baseline POINTS="114,764 1512,769"/>
            <String CONTENT="che picciola parte de i molti obligi, ch’io le tengo, che nõ"/>
          </TextLine>
          <TextLine HPOS="113" VPOS="786" WIDTH="1399" HEIGHT="64">
            <Baseline POINTS="113,833 1512,833"/>
            <String CONTENT="hauendo per hora cosa più à proposito della presente Ico¬"/>
          </TextLine>
          <TextLine HPOS="110" VPOS="852" WIDTH="1404" HEIGHT="65">
            <Baseline POINTS="111,899 1514,902"/>
            <String CONTENT="nologia del Cauallier Cesare Ripa, Libro di gran nome, &amp;"/>
          </TextLine>
          <TextLine HPOS="1467" VPOS="934" WIDTH="5" HEIGHT="24">
            <Baseline POINTS="1467,950 1472,950"/>
            <String CONTENT="2"/>
          </TextLine>
          <TextLine HPOS="110" VPOS="922" WIDTH="1391" HEIGHT="62">
            <Baseline POINTS="111,965 1501,968"/>
            <String CONTENT="di molto spaccio, ristampato da me nuouamente, &amp; accré¬"/>
          </TextLine>
          <TextLine HPOS="107" VPOS="985" WIDTH="1402" HEIGHT="64">
            <Baseline POINTS="108,1033 993,1033 1509,1037"/>
            <String CONTENT="sciuto dall’Auttore medesimo, di molte figure, &amp; discorsi,"/>
          </TextLine>
          <TextLine HPOS="110" VPOS="1055" WIDTH="1402" HEIGHT="58">
            <Baseline POINTS="111,1100 1512,1102"/>
            <String CONTENT="che mancauano in tutte l’altre editioni, hò deliberato di"/>
          </TextLine>
          <TextLine HPOS="110" VPOS="1120" WIDTH="1402" HEIGHT="63">
            <Baseline POINTS="110,1167 1512,1167"/>
            <String CONTENT="adornarlo del glorioso nome di V.P. molto Reuerenda, si"/>
          </TextLine>
          <TextLine HPOS="653" VPOS="1168" WIDTH="9" HEIGHT="25">
            <Baseline POINTS="655,1185 662,1186"/>
            <String CONTENT=""/>
          </TextLine>
          <TextLine HPOS="113" VPOS="1188" WIDTH="1398" HEIGHT="63">
            <Baseline POINTS="114,1234 1511,1233"/>
            <String CONTENT="per dare al Libro riputatione, si per notificare al Mondo"/>
          </TextLine>
          <TextLine HPOS="113" VPOS="1239" WIDTH="33" HEIGHT="22">
            <Baseline POINTS="113,1251 146,1251"/>
            <String CONTENT="1"/>
          </TextLine>
          <TextLine HPOS="112" VPOS="1258" WIDTH="1395" HEIGHT="61">
            <Baseline POINTS="112,1300 1507,1300"/>
            <String CONTENT="quant io tenga a mente le gratie, di che ella in diuerse oc¬"/>
          </TextLine>
          <TextLine HPOS="111" VPOS="1322" WIDTH="1396" HEIGHT="61">
            <Baseline POINTS="112,1367 1507,1370"/>
            <String CONTENT="casioni m’ha fauorito, le piacera accetarlo con la fronte"/>
          </TextLine>
          <TextLine HPOS="109" VPOS="1389" WIDTH="1396" HEIGHT="63">
            <Baseline POINTS="110,1434 1505,1435"/>
            <String CONTENT="lieta, ch’io con cuore pronto glielo dono, &amp; con farli ri¬"/>
          </TextLine>
          <TextLine HPOS="110" VPOS="1455" WIDTH="1391" HEIGHT="72">
            <Baseline POINTS="111,1501 1106,1505 1501,1502"/>
            <String CONTENT="uerenria finisco. Della mia Libreria il dì 10.Genn.1618."/>
          </TextLine>
