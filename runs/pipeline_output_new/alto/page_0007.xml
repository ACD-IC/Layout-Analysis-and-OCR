          <TextLine HPOS="533" VPOS="942" WIDTH="966" HEIGHT="59">
            <Baseline POINTS="534,986 1116,986 1499,981"/>
            <String CONTENT="R A quanti amici, &amp; patroniio m’hab¬"/>
          </TextLine>
          <TextLine HPOS="552" VPOS="1011" WIDTH="950" HEIGHT="47">
            <Baseline POINTS="553,1053 1502,1052"/>
            <String CONTENT="bia mai hauuto, io non ho in alcun tem"/>
          </TextLine>
          <TextLine HPOS="553" VPOS="1073" WIDTH="952" HEIGHT="59">
            <Baseline POINTS="554,1121 1150,1118 1505,1120"/>
            <String CONTENT="po conosciuto persona, che habbia piu"/>
          </TextLine>
          <TextLine HPOS="556" VPOS="1123" WIDTH="17" HEIGHT="19">
            <Baseline POINTS="557,1135 573,1137"/>
            <String CONTENT="L"/>
          </TextLine>
          <TextLine HPOS="549" VPOS="1140" WIDTH="958" HEIGHT="60">
            <Baseline POINTS="550,1187 1507,1184"/>
            <String CONTENT="amabili qualita, &amp; che mi voglia piu"/>
          </TextLine>
          <TextLine HPOS="550" VPOS="1204" WIDTH="958" HEIGHT="61">
            <Baseline POINTS="551,1255 1508,1251"/>
            <String CONTENT="bene, di V.P. molto Reu. Et per lascia"/>
          </TextLine>
          <TextLine HPOS="546" VPOS="1274" WIDTH="963" HEIGHT="60">
            <Baseline POINTS="546,1325 585,1322 777,1320 1509,1319"/>
            <String CONTENT="re da parte la intelligenza bona, ch’ella"/>
          </TextLine>
          <TextLine HPOS="104" VPOS="1339" WIDTH="1404" HEIGHT="56">
            <Baseline POINTS="105,1387 608,1389 1508,1385"/>
            <String CONTENT="ha di Arithmetica, di Geometria, d’Architetura, di Mu¬"/>
          </TextLine>
          <TextLine HPOS="104" VPOS="1409" WIDTH="1405" HEIGHT="62">
            <Baseline POINTS="105,1456 1509,1454"/>
            <String CONTENT="sica, di formare con la penna ogni sorte di politi caratteri,"/>
          </TextLine>
          <TextLine HPOS="104" VPOS="1476" WIDTH="1404" HEIGHT="58">
            <Baseline POINTS="105,1524 1508,1521"/>
            <String CONTENT="che tutte sono arti nobili, &amp; atte ad illustrare ogn' vna per"/>
          </TextLine>
          <TextLine HPOS="1247" VPOS="1522" WIDTH="29" HEIGHT="22">
            <Baseline POINTS="1247,1537 1276,1537"/>
            <String CONTENT="O"/>
          </TextLine>
          <TextLine HPOS="105" VPOS="1544" WIDTH="1408" HEIGHT="58">
            <Baseline POINTS="106,1590 1513,1588"/>
            <String CONTENT="se vn’homo; chi può vedere &amp; non amare in lei la destrez"/>
          </TextLine>
          <TextLine HPOS="108" VPOS="1608" WIDTH="1401" HEIGHT="62">
            <Baseline POINTS="109,1658 1509,1656"/>
            <String CONTENT="za, la diligenza, l’affabilit à, &amp; l’altre notabili sue parti ?"/>
          </TextLine>
          <TextLine HPOS="1351" VPOS="1662" WIDTH="18" HEIGHT="18">
            <Baseline POINTS="1353,1675 1369,1673"/>
            <String CONTENT="4"/>
          </TextLine>
          <TextLine HPOS="112" VPOS="1675" WIDTH="1393" HEIGHT="66">
            <Baseline POINTS="112,1724 729,1726 1505,1722"/>
            <String CONTENT="Queste sono state le cagioni, che V. P. s’habbia acquista¬"/>
          </TextLine>
          <TextLine HPOS="110" VPOS="1744" WIDTH="1391" HEIGHT="54">
            <Baseline POINTS="110,1792 1501,1790"/>
            <String CONTENT="to l’amore di tutti i suoi Padri di cotesta nobilissima Con¬"/>
          </TextLine>
          <TextLine HPOS="116" VPOS="1814" WIDTH="1379" HEIGHT="61">
            <Baseline POINTS="117,1859 1495,1858"/>
            <String CONTENT="gregatione di Santa Giustina, che sia stata sempre impie"/>
          </TextLine>
          <TextLine HPOS="113" VPOS="1856" WIDTH="43" HEIGHT="26">
            <Baseline POINTS="114,1875 156,1875"/>
            <String CONTENT="O"/>
          </TextLine>
          <TextLine HPOS="116" VPOS="1882" WIDTH="1398" HEIGHT="57">
            <Baseline POINTS="116,1926 1514,1926"/>
            <String CONTENT="gata in maneggrimportantissimi, come sarebbe a dire nel"/>
          </TextLine>
          <TextLine HPOS="116" VPOS="1927" WIDTH="16" HEIGHT="22">
            <Baseline POINTS="116,1941 132,1941"/>
            <String CONTENT="C"/>
          </TextLine>
          <TextLine HPOS="430" VPOS="1927" WIDTH="52" HEIGHT="22">
            <Baseline POINTS="430,1942 482,1942"/>
            <String CONTENT="O0"/>
          </TextLine>
          <TextLine HPOS="115" VPOS="1948" WIDTH="1400" HEIGHT="57">
            <Baseline POINTS="115,1993 1515,1992"/>
            <String CONTENT="Cellerariato di Correggiola, &amp; che fin almente doppo mol"/>
          </TextLine>
          <TextLine HPOS="639" VPOS="1993" WIDTH="40" HEIGHT="26">
            <Baseline POINTS="639,2009 679,2009"/>
            <String CONTENT="20"/>
          </TextLine>
          <TextLine HPOS="1346" VPOS="1992" WIDTH="9" HEIGHT="27">
            <Baseline POINTS="1346,2011 1355,2011"/>
            <String CONTENT="1"/>
          </TextLine>
          <TextLine HPOS="111" VPOS="2013" WIDTH="1404" HEIGHT="64">
            <Baseline POINTS="112,2058 1208,2062 1515,2058"/>
            <String CONTENT="to trauagliare, i suoi Superiori habbiano anteposto il suo"/>
          </TextLine>
          <TextLine HPOS="112" VPOS="2079" WIDTH="1401" HEIGHT="61">
            <Baseline POINTS="113,2126 994,2131 1513,2126"/>
            <String CONTENT="gusto, &amp; la sua quiete, al bisogno, ch’essi teneuano della"/>
          </TextLine>
          <TextLine HPOS="114" VPOS="2127" WIDTH="24" HEIGHT="20">
            <Baseline POINTS="115,2141 138,2140"/>
            <String CONTENT=""/>
          </TextLine>
          <TextLine HPOS="105" VPOS="2146" WIDTH="1384" HEIGHT="61">
            <Baseline POINTS="106,2194 907,2199 1489,2193"/>
            <String CONTENT="sua sempre lodata sufficienza. E veramente non è da ne"/>
          </TextLine>
