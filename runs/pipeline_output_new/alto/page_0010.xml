        <TextBlock ID="region_1_MainZone-Continued" HPOS="174" VPOS="276" WIDTH="1435" HEIGHT="1715" TAGREFS="MainZone-Continued">
          <TextLine HPOS="201" VPOS="276" WIDTH="1388" HEIGHT="40">
            <Baseline POINTS="202,299 934,284 1589,282"/>
            <String CONTENT="Ripá, e talatala julor degitrrori avisa culriene Rtomana, ce.-"/>
          </TextLine>
          <TextLine HPOS="199" VPOS="303" WIDTH="1392" HEIGHT="80">
            <Baseline POINTS="200,365 941,351 1591,349"/>
            <String CONTENT="quella diligenza, che piacque fino all’Auttore medesimo, come si"/>
          </TextLine>
          <TextLine HPOS="202" VPOS="372" WIDTH="1390" HEIGHT="78">
            <Baseline POINTS="203,431 895,418 1592,414"/>
            <String CONTENT="vederà per lo transunto d’vna sua Lettera, che qui sotto si re¬"/>
          </TextLine>
          <TextLine HPOS="203" VPOS="437" WIDTH="1389" HEIGHT="80">
            <Baseline POINTS="204,498 938,485 1592,482"/>
            <String CONTENT="sistrera; venne in pensiero a chi s’è preso cura di ristampare la"/>
          </TextLine>
          <TextLine HPOS="200" VPOS="502" WIDTH="1392" HEIGHT="82">
            <Baseline POINTS="201,567 900,553 1592,549"/>
            <String CONTENT="medesima opera in Siena, di biasimare quanto d’industria io ado¬"/>
          </TextLine>
          <TextLine HPOS="197" VPOS="572" WIDTH="1395" HEIGHT="79">
            <Baseline POINTS="198,634 845,620 1592,618"/>
            <String CONTENT="perai all’hora per cauare questa nobile opera fuor delle miserie del.."/>
          </TextLine>
          <TextLine HPOS="197" VPOS="640" WIDTH="1394" HEIGHT="76">
            <Baseline POINTS="198,702 900,688 1591,684"/>
            <String CONTENT="la primiera editionc. Non voglio pero io imitare chi ha fatto"/>
          </TextLine>
          <TextLine HPOS="198" VPOS="713" WIDTH="1392" HEIGHT="70">
            <Baseline POINTS="199,769 772,757 1590,750"/>
            <String CONTENT="ma le, et rendere come si dice pane per focaccia, ma mi contenta¬"/>
          </TextLine>
          <TextLine HPOS="197" VPOS="774" WIDTH="1393" HEIGHT="73">
            <Baseline POINTS="198,835 747,824 1590,818"/>
            <String CONTENT="rò di dire, che la editione Romana, et la Stampa di Siena hanno"/>
          </TextLine>
          <TextLine HPOS="196" VPOS="842" WIDTH="1395" HEIGHT="78">
            <Baseline POINTS="196,902 827,891 1591,886"/>
            <String CONTENT="fra se quella relatione, che tiene il male al peggio. Questo mio"/>
          </TextLine>
          <TextLine HPOS="194" VPOS="909" WIDTH="1398" HEIGHT="72">
            <Baseline POINTS="194,970 916,957 1592,954"/>
            <String CONTENT="detto so, ch’è notorio, &amp;r che non sarà negato se non da chi nega"/>
          </TextLine>
          <TextLine HPOS="196" VPOS="972" WIDTH="1395" HEIGHT="81">
            <Baseline POINTS="196,1038 772,1024 1591,1019"/>
            <String CONTENT="hauendo occhi, che’l'Sole habbia luce. Sò appresso, che persone"/>
          </TextLine>
          <TextLine HPOS="196" VPOS="1040" WIDTH="1398" HEIGHT="83">
            <Baseline POINTS="197,1103 932,1089 1594,1086"/>
            <String CONTENT="molto intendenti, che s’hanno preso briga di aggiustare i spropositi"/>
          </TextLine>
          <TextLine HPOS="196" VPOS="1110" WIDTH="1398" HEIGHT="76">
            <Baseline POINTS="196,1171 826,1158 1594,1153"/>
            <String CONTENT="certi in questa terza stampa, non per colpa dell’Auttore, ma di"/>
          </TextLine>
          <TextLine HPOS="194" VPOS="1176" WIDTH="1386" HEIGHT="76">
            <Baseline POINTS="195,1237 802,1224 1580,1223"/>
            <String CONTENT="persone poco amoreuoli hanno durato fatica degna di molta lode, (er"/>
          </TextLine>
          <TextLine HPOS="194" VPOS="1249" WIDTH="1397" HEIGHT="62">
            <Baseline POINTS="195,1304 778,1292 1591,1286"/>
            <String CONTENT="hanno hauuto a combattere con facciate, &amp;t quinterni intieri tra¬"/>
          </TextLine>
          <TextLine HPOS="197" VPOS="1311" WIDTH="1398" HEIGHT="73">
            <Baseline POINTS="198,1370 702,1361 1595,1355"/>
            <String CONTENT="sposti, con errori di testo, &amp; d’auttorita incomportabili, con figu¬"/>
          </TextLine>
          <TextLine HPOS="194" VPOS="1381" WIDTH="1400" HEIGHT="75">
            <Baseline POINTS="194,1438 820,1426 1594,1423"/>
            <String CONTENT="re shalzate, con dipinture di parole greche in caratteri latini, con"/>
          </TextLine>
          <TextLine HPOS="259" VPOS="1442" WIDTH="1267" HEIGHT="77">
            <Baseline POINTS="260,1504 830,1493 1526,1487"/>
            <String CONTENT="l’autorità latine scorrettissime, &amp; con cose simili, che posso¬"/>
          </TextLine>
          <TextLine HPOS="330" VPOS="1514" WIDTH="1122" HEIGHT="76">
            <Baseline POINTS="331,1568 1452,1556"/>
            <String CONTENT="no eßere riconosciute da qual si voglia, che non voglia"/>
          </TextLine>
          <TextLine HPOS="396" VPOS="1582" WIDTH="992" HEIGHT="72">
            <Baseline POINTS="397,1633 1388,1624"/>
            <String CONTENT="cosi di leggiero credere a me, quando si rissolue¬"/>
          </TextLine>
          <TextLine HPOS="467" VPOS="1650" WIDTH="851" HEIGHT="66">
            <Baseline POINTS="468,1702 1318,1691"/>
            <String CONTENT="ra di prendere il libro in mano, et an¬"/>
          </TextLine>
          <TextLine HPOS="529" VPOS="1718" WIDTH="719" HEIGHT="61">
            <Baseline POINTS="530,1767 1096,1761 1248,1764"/>
            <String CONTENT="darlo scorrendo. Ma tanto basti,"/>
          </TextLine>
          <TextLine HPOS="601" VPOS="1789" WIDTH="585" HEIGHT="61">
            <Baseline POINTS="602,1837 842,1830 1186,1829"/>
            <String CONTENT="&amp; parli per me l’Aluttore"/>
          </TextLine>
          <TextLine HPOS="678" VPOS="1856" WIDTH="436" HEIGHT="60">
            <Baseline POINTS="678,1896 1114,1896"/>
            <String CONTENT="medesimo, ilquale in"/>
          </TextLine>
          <TextLine HPOS="746" VPOS="1926" WIDTH="303" HEIGHT="54">
            <Baseline POINTS="747,1962 837,1966 1049,1961"/>
            <String CONTENT="vna sua Let¬"/>
          </TextLine>
